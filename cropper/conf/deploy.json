{
    "packages": {
        "media-service-cropper": {
            "type": "autoscaling",
            "data": {
                "port": 9000,
                "bucket": "media-service-dist",
                "publicReadAcl": false
            }
        }
    },
    "recipes": {
        "default": {
            "depends": ["artifactUploadOnly", "deployOnly"]
        },
        "deployOnly": {
            "actionsBeforeApp": ["media-service-cropper.deploy"]
        },
        "artifactUploadOnly": {
            "actionsBeforeApp": ["media-service-cropper.uploadArtifacts"]
        }
    }
}
