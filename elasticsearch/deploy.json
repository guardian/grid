{
    "packages": {
        "elasticsearch": {
            "type": "elasticsearch",
            "data": {
                "bucket": "media-service-dist"
            }
        }
    },
    "recipes": {
        "default": {
            "depends": ["artifactUploadOnly", "deployOnly"]
        },
        "deployOnly": {
            "actionsBeforeApp": ["elasticsearch.deploy"]
        },
        "artifactUploadOnly": {
            "actionsBeforeApp": ["elasticsearch.uploadArtifacts"]
        }
    }
}
