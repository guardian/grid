{
  "name": "image-embedder-lambda",
  "version": "1.0.0",
  "description": "This is a service responsible for embedding uploaded images to the Grid and writing those embeddings to the S3 Vector Store.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "type-check": "tsc --noEmit",
    "bundle": "esbuild src/index.ts --bundle --platform=node --target=node24 --format=cjs --outfile=dist/index.js",
    "build": "run-p --print-label type-check bundle",
    "local": "ts-node local-runner.ts"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/aws-lambda": "^8.10.159",
    "@types/node": "^25.0.0",
    "esbuild": "0.27.0",
    "npm-run-all": "^4.1.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@aws-sdk/client-bedrock-runtime": "^3.948.0",
    "@aws-sdk/client-s3": "^3.948.0",
    "@aws-sdk/client-s3vectors": "^3.952.0",
    "@aws-sdk/client-sqs": "^3.948.0"
  }
}
